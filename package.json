{
  "name": "water_widget",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://www.bolius.dk/fileadmin/apps/water_comes/yougov/",
  "dependencies": {
    "@sentry/browser": "^5.9.1",
    "bootstrap": "^4.4.0",
    "dawa-autocomplete2": "^1.0.2",
    "npm": "^6.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-responsive-modal": "^4.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^2.4.3",
    "react-spinners": "^0.6.1",
    "react-test-renderer": "^16.12.0",
    "reactstrap": "^8.1.1",
    "styled-components": "^4.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false",
    "coveralls": "cat coverage/lcov.info | node_modules/.bin/coveralls",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/jest": "^24.0.23",
    "babel-plugin-styled-components": "^1.10.6",
    "webpack-cli": "^3.3.10"
  },
  "description": "A widget made for the _when the water comes_ campaign.",
  "main": "webpack.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bolius/water_comes.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bolius/water_comes/issues"
  }
}
